@model System.Collections.Generic.IEnumerable<IntegrationServicesHub.Models.HubModels.ContentAgreement>

@{
    ViewBag.Title = "Index";
}

<ul class="nav navbar-nav">
    <li>@Html.ActionLink("Service Providers List", "Index", "ServiceProviders")</li>
    <li>@Html.ActionLink("Content Agreements List", "Index", "ContentAgreements")</li>
    <li>@Html.ActionLink("Content Rights Owners List", "Index", "ContentsRightsOwner")</li>
    <li>@Html.ActionLink("View Matchrule list", "Index", "Home")</li>
    <li>@Html.ActionLink("Multiple Service Prices List", "Index", "Home")</li>
    <li>@Html.ActionLink("Create New", "Create")</li>
</ul>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ObjectID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContentAgreementName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ObjectID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContentAgreementName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
